{
    "name": "theme",
    "version": "4.2.0",
    "description": "Theme assets for noise-noise Django project",
    "scripts": {
        "start": "npm run dev",
        "build": "npm run build:clean && npm run build:tailwind",
        "build:clean": "rimraf ../static/css/dist",
        "build:tailwind": "cross-env NODE_ENV=production postcss ./src/styles.css -o ../static/css/dist/styles.css --minify",
        "dev": "cross-env NODE_ENV=development postcss ./src/styles.css -o ../static/css/dist/styles.css --watch",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.11",
        "autoprefixer": "^10.4.21",
        "daisyui": "^5.1.6",
        "cross-env": "^7.0.3",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "postcss-nested": "^7.0.2",
        "postcss-simple-vars": "^7.0.1",
        "rimraf": "^6.0.1",
        "tailwindcss": "^4.1.13"
    },
    "dependencies": {
        "@tailwindcss/cli": "^4.1.12",
        "@tailwindcss/postcss": "^4.1.12",
        "daisyui": "^5.1.6",
        "postcss-cli": "^11.0.1",
        "theme-change": "^2.5.0"
    }
}
